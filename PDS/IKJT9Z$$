//TEST300  JOB (),'TEST SUBCMD INSTALL',CLASS=A,MSGCLASS=H,
//             REGION=6M,NOTIFY=&SYSUID,COND=(0,NE)
//*
//* ********************************************************
//* *  INSTALL SUBCOMMANDS OF Z/OS TEST                    *
//* ********************************************************
//*
//*   FIRSTLY, VERIFY THAT YOU ARE HAPPY WITH
//*   (A) THE NAMES OF THE PROGRAMS TO BE INSTALLED, AND
//*   (B) THE NAMES OF THE SUBCOMMANDS TO BE INSTALLED.
//*
//*   IF NOT, THEN ALTER THE NAMES TO SUIT YOUR INSTALLATION.
//*
//*   IF APPROPRIATE, REFRESH LLA AFTERWARDS.
//*
//*   IF UPDATING PARMLIB THEN
//*   (A) SKIP THE BATCH STUFF AND EDIT THE RELEVANT MEMBER DIRECTLY.
//*   (B) IF RUNNING IN BATCH THEN CHECK THE PARMLIB DATA SET NAME
//*       AND THE MEMBER NAME(S).
//*   (C) ISSUE THE TSO COMMAND
//*           PARMLIB UPD(N0)
//*       AFTERWARDS - CHANGING 'N0' TO THE RELEVANT MEMBER NAME
//*       SUFFIX - TO ACTIVATE THE NEW PARAMETERS.
//*
//ASMLK    PROC MEM=DUMMY
//*
//ASM      EXEC  PGM=ASMA90,REGION=32M,
//             PARM=(NORLD,RENT,TERM,OBJ,
//             'XREF(SHORT,UNREFS),SIZE(MAX,ABOVE)',
//             'FLAG(PAGE0)')
//SYSUT1   DD  UNIT=VIO,SPACE=(4096,(120,120),,,ROUND),DCB=BUFNO=1
//SYSLIN   DD  UNIT=VIO,SPACE=(TRK,(5,5)),DISP=(MOD,PASS),DSN=&&OBJ,
//             DCB=BLKSIZE=3120
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSIN    DD  DISP=SHARE,DSN=YOUR.CBT300.LIBRARY(&MEM.)
//SYSLIB   DD  DSN=SYS1.MODGEN,DISP=SHARE
//         DD  DSN=SYS1.MACLIB,DISP=SHARE
//*
//LKED     EXEC  PGM=IEWL,REGION=32M,
//             PARM='MAP,RENT,REUS,REFR,NCAL'
//SYSPRINT DD  SYSOUT=*
//SYSLMOD  DD  DISP=SHARE,DSN=YOUR.LINKLIST.LIBRARY(&MEM.)
//SYSLIN   DD  DSN=&&OBJ,DISP=(OLD,DELETE)
//         PEND
//*
//STEP1    EXEC ASMLK,MEM=IKJT9LBZ
//STEP2    EXEC ASMLK,MEM=IKJT9FIZ
//STEP3    EXEC ASMLK,MEM=IKJT9LGZ
//
//*  (NOTE THE NULL CARD ABOVE)
//*
//*  REALLY, JUST USE ISPF EDIT TO DO THIS BIT:
//*
//UPTPARM  EXEC PGM=IEBGENER
//SYSPRINT  DD  SYSOUT=*
//SYSUT1    DD  DISP=SHR,DSN=SYS1.PARMLIB(IKJTSO00)
//          DD  *

TEST   TSOCMD(TSOEXEC)            /* GIVE ACCESS TO TSO COMMANDS   */ +
       SUBCMD((F,IKJT9FIZ),       /* FIND    (Z/OS LEVEL)          */ +
              (FIND,IKJT9FIZ),    /* FIND    (Z/OS LEVEL)          */ +
              (LB,IKJT9LBZ),      /* LISTBRK (Z/OS LEVEL)          */ +
              (LG,IKJT9LGZ))      /* LISTGPR (Z/OS LEVEL)          */

